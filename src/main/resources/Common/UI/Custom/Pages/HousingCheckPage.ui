$C = "../Common.ui";

$C.@PageOverlay {}

$C.@DecoratedContainer {
  Anchor: (Width: 480, Height: 400);

  #Title {
    Group {
      $C.@Title {
        @Text = "Housing Check";
      }
    }
  }

  #Content {
    Group #ElementList {
      LayoutMode: TopScrolling;
      ScrollbarStyle: $C.@DefaultScrollbarStyle;
      Padding: (Full: 12);
    }
  }
}

$C.@BackButton {}
